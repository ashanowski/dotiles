#!/bin/bash
    f=3
    b=4

    for j in f b; do
        for i in {0..7}; do
            printf -v $j$i %b "\033[${!j}${i}m"
        done
    done

    bld=$'\033[1m'
    rst=$'\033[0m'
    inv=$'\033[7m'

cat << EOF

$f1      __        $f2      __        $f3      __        $f4      __        $f5      __        $f6      __        
$f1     /\_\       $f2     /\_\       $f3     /\_\       $f4     /\_\       $f5     /\_\       $f6     /\_\       
$f1    / / /_      $f2    / / /_      $f3    / / /_      $f4    / / /_      $f5    / / /_      $f6    / / /_      
$f1   / /_/\ \     $f2   / /_/\ \     $f3   / /_/\ \     $f4   / /_/\ \     $f5   / /_/\ \     $f6   / /_/\ \     
$f1  _\ \/  \ \    $f2  _\ \/  \ \    $f3  _\ \/  \ \    $f4  _\ \/  \ \    $f5  _\ \/  \ \    $f6  _\ \/  \ \    
$f1 /\ \  /\ \_\   $f2 /\ \  /\ \_\   $f3 /\ \  /\ \_\   $f4 /\ \  /\ \_\   $f5 /\ \  /\ \_\   $f6 /\ \  /\ \_\   
$f1 \ \/  \ \/_/   $f2 \ \/  \ \/_/   $f3 \ \/  \ \/_/   $f4 \ \/  \ \/_/   $f5 \ \/  \ \/_/   $f6 \ \/  \ \/_/   
$f1  \  /\ \_\     $f2  \  /\ \_\     $f3  \  /\ \_\     $f4  \  /\ \_\     $f5  \  /\ \_\     $f6  \  /\ \_\     
$f1   \/_/ / /     $f2   \/_/ / /     $f3   \/_/ / /     $f4   \/_/ / /     $f5   \/_/ / /     $f6   \/_/ / /     
$f1     / / /      $f2     / / /      $f3     / / /      $f4     / / /      $f5     / / /      $f6     / / /      
$f1     \/_/       $f2     \/_/       $f3     \/_/       $f4     \/_/       $f5     \/_/       $f6     \/_/       
$bld
$f1      __        $f2      __        $f3      __        $f4      __        $f5      __        $f6      __        
$f1     /\_\       $f2     /\_\       $f3     /\_\       $f4     /\_\       $f5     /\_\       $f6     /\_\       
$f1    / / /_      $f2    / / /_      $f3    / / /_      $f4    / / /_      $f5    / / /_      $f6    / / /_      
$f1   / /_/\ \     $f2   / /_/\ \     $f3   / /_/\ \     $f4   / /_/\ \     $f5   / /_/\ \     $f6   / /_/\ \     
$f1  _\ \/  \ \    $f2  _\ \/  \ \    $f3  _\ \/  \ \    $f4  _\ \/  \ \    $f5  _\ \/  \ \    $f6  _\ \/  \ \    
$f1 /\ \  /\ \_\   $f2 /\ \  /\ \_\   $f3 /\ \  /\ \_\   $f4 /\ \  /\ \_\   $f5 /\ \  /\ \_\   $f6 /\ \  /\ \_\   
$f1 \ \/  \ \/_/   $f2 \ \/  \ \/_/   $f3 \ \/  \ \/_/   $f4 \ \/  \ \/_/   $f5 \ \/  \ \/_/   $f6 \ \/  \ \/_/   
$f1  \  /\ \_\     $f2  \  /\ \_\     $f3  \  /\ \_\     $f4  \  /\ \_\     $f5  \  /\ \_\     $f6  \  /\ \_\     
$f1   \/_/ / /     $f2   \/_/ / /     $f3   \/_/ / /     $f4   \/_/ / /     $f5   \/_/ / /     $f6   \/_/ / /     
$f1     / / /      $f2     / / /      $f3     / / /      $f4     / / /      $f5     / / /      $f6     / / /      
$f1     \/_/       $f2     \/_/       $f3     \/_/       $f4     \/_/       $f5     \/_/       $f6     \/_/       
$rst
EOF